# Archivo: .env.example

# Puerto del servidor
PORT=3000

# Cadena de conexión a MongoDB
MONGO_URI=mongodb+srv://<usuario>:<contraseña>@cluster0.mongodb.net/studyswap

# Clave secreta para JWT
JWT_SECRET=studyswap-secret

# AWS/Firebase (si lo usas para subir archivos)
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
S3_BUCKET_NAME=...

# Opcional: clave API para Algolia, ElasticSearch o Notion
ALGOLIA_APP_ID=...
ALGOLIA_API_KEY=...
